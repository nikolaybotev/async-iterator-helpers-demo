{
  "babelrc": false,
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "0.10" // or specify your target environment
        },
        "debug": true
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "absoluteRuntime": false,
        "corejs": false,
        "helpers": true,
        "regenerator": true
      }
    ]
  ],
  "sourceMaps": "inline",
  "sourceType": "module"
}
